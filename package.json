{
  "name": "crutchai_backend",
  "version": "1.0.0",
  "description": "Backend API for CrutchAI application with authentication and Supabase integration",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "docs": "npx swagger-jsdoc -d src/docs/swaggerDef.ts -o src/docs/swagger.json",
    "test": "echo \"Error: no tests configured yet\" && exit 1"
  },
  "keywords": [
    "express",
    "api",
    "authentication",
    "supabase",
    "oauth"
  ],
  "author": "CrutchAI Team",
  "license": "ISC",
  "dependencies": {
    "@auth/core": "^0.39.1",
    "@supabase/supabase-js": "^2.49.4",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.3",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.10.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.21",
    "@types/helmet": "^0.0.48",
    "@types/node": "^20.11.30",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
